<h1>Appointment</h1>
<fieldset>
    <div class="row">
        <div class="col-md-10 mp0i">
            <div class="col-xs-9 pull-left">
                <h2>Appointment Information</h2>
            </div>
            <div class="col-xs-3 pull-right">
              <%= select_tag("", options_for_select(['New Patient', 'Existing Patient'], :selected => "New"), { :id => 'fld-option', :class => 'form-control m-b' }) %>
            </div>
        </div>
    </div>
  <div class="row">
    <input type="hidden" id="hdn-fld-patients" value="<%= @patients.to_json %>">
    <div class="col-md-10">

    <div class="form-group">
        <div class="row">
                <%= hidden_field_tag "patient[appointments][patient_id]", 0, :class => "form-control", :data => { :name => "patient-id" } %>
                <div id="patient-name-container" >

                </div>
            </div>
      </div>

      <div class="form-group">
        <div class="row">
          <div class="col-xs-9">
            <label class="control-label lbl-consultation-date">Consultation Date *</label>
            <div id="fld-consultation-date">
              <div class="input-group date">
                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                <%= text_field_tag "patient[appointments][consultation_date]", DateTime.now.to_date.strftime('%d/%m/%Y'), :class => "form-control required", :data => { :name => "consultation-date" } %>
              </div>
            </div>
          </div>
          <div class="col-xs-3">
            <label class="control-label">Type <large>*</large></label>
            <%= select_tag("patient[appointments][medical_records][type]", options_for_select(Appointment.medical_record_types.collect{ |k, v| [k.humanize.upcase, k]}), { :class => 'form-control m-b' }) %>
          </div>
        </div>
      </div>


      <div class="form-group">
        <div class="row">
          <div class="col-xs-4">
            <label class="control-label lbl-lmp">LMP *</label>
            <div id="date_lmp">
              <div class="input-group date">
                <span class="input-group-addon"><i class="fa fa-calendar"></i></span><%= text_field_tag "patient[appointments][medical_records][lmp]", "",  autofocus: true, class: "form-control", :data => { :name => "lmp" } %>
              </div>
            </div>
          </div>
          <div id="edc-container" class="col-xs-4">
            <label class="control-label">EDC</label>
            <input id="fld-edc-display" type="text" class="form-control" disabled value="" />
            <%= hidden_field_tag "patient[appointments][medical_records][edc]", "", class: "form-control"%>
          </div>
          <div id="aog-container" class="col-xs-4">
            <label class="control-label">AOG</label>
            <input id="fld-aog-display" type="text" class="form-control" disabled value="" />
            <%= hidden_field_tag "patient[appointments][medical_records][aog]", "", class: "form-control"%>
          </div>
        </div>
      </div>

      <div class="form-group">
        <div class="row">
          <div class="col-md-4">
            <label class="control-label lbl-systolic">Systolic *</label>
            <%= text_field_tag "patient[appointments][systolic]", "", :class => "form-control required", :data => { :name => "systolic" } %>
          </div>
          <div class="col-md-4">
            <label class="control-label lbl-diastolic">Diastolic *</label>
            <%= text_field_tag "patient[appointments][diastolic]", "", :class => "form-control required", :data => { :name => "diastolic" } %>
          </div>
          <div class="col-md-4">
            <label class="control-label lbl-weight">Weight (kg) *</label>
            <%= text_field_tag "patient[appointments][weight]", "", :class => "form-control required", :data => { :name => "weight" } %>
          </div>
        </div>
      </div>

      <div class="form-group">
        <div class="row">
          <div class="col-xs-12">
            <label class="control-label lbl-chief-complaint">Chief Complaint <large class="required">*</large></label>
            <%= text_area_tag "patient[appointments][medical_records][chief_complaint]", "", :class => "form-control", :style => "height: 100px;", :required => true, :data => { :name => "chief-complaint" } %>
          </div>
        </div>
      </div>

      <div class="hr-line-dashed"></div>

      <div class="form-group">
        <div class="row">
          <div class="col-xs-2">
            <label class="control-label">Menarche</label>
            <%= select_tag("patient[appointments][medical_records][menarche]", options_for_select(0..20, 0), :class => "form-control m-b") %>
          </div>
          <div class="col-xs-2">
            <label class="control-label">GRAVIDA</label>
            <%= select_tag("patient[appointments][medical_records][gravida]", options_for_select(0..20, 0), :class => 'form-control m-b') %>
          </div>
          <div class="col-xs-2">
            <label class="control-label">PARA</label>
            <%= select_tag("patient[appointments][medical_records][para]", options_for_select(0..20, 0), :class => 'form-control m-b') %>
          </div>
          <div class="col-xs-1" style="padding-right: 0;">
            <label class="control-label">T</label>
            <%= select_tag("patient[appointments][medical_records][t]", options_for_select(0..20, 0), :class => 'form-control m-b') %>
          </div>
          <div class="col-xs-1" style="padding-right: 0;">
            <label class="control-label">P</label>
            <%= select_tag("patient[appointments][medical_records][p]", options_for_select(0..20, 0), :class => 'form-control m-b') %>
          </div>
          <div class="col-xs-1" style="padding-right: 0;">
            <label class="control-label">A</label>
            <%= select_tag("patient[appointments][medical_records][a]", options_for_select(0..20, 0), :class => 'form-control m-b') %>
          </div>
          <div class="col-xs-1" style="padding-right: 0;">
            <label class="control-label">L</label>
            <%= select_tag("patient[appointments][medical_records][l]", options_for_select(0..20, 0), :class => 'form-control m-b') %>
          </div>
        </div>
      </div>





      <div class="row">

        <div class="col-md-12">

          <div class="form-group">
            <div class="row">
              <div class="col-xs-12">
                <label class="control-label">History Of Present Illness</label>
                <%= text_area_tag "patient[appointments][medical_records][history_of_present_illness]", "", :class => "form-control", :style => "height: 100px;" %>
              </div>
            </div>
          </div>
          <div class="form-group">
            <div class="row">
              <div class="col-xs-12">
                <label class="control-label lbl-ob-history">Ob-Gyne History</label>
                <%= text_area_tag "patient[appointments][medical_records][ob_history]", "", :class => "form-control", :style => "height: 100px;", :data => { :name => "ob-history" }%>
              </div>
            </div>
          </div>
          <div class="form-group">
            <div class="row">
              <div class="col-xs-12">
                <label class="control-label lbl-ob-history">Diagnosis</label>
                <%= text_area_tag "patient[appointments][medical_records][diagnosis]", "", :class => "form-control", :style => "height: 100px;", :data => { :name => "ob-history" }%>
              </div>
            </div>
          </div>
          <div class="form-group">
            <div class="row">
              <div class="col-xs-12">
                <label class="control-label">Past Medical History</label>
                <%= text_area_tag "patient[appointments][medical_records][past_medical_history]", "", :class => "form-control", :style => "height: 100px;" %>
              </div>
            </div>
          </div>

          <div class="form-group">
            <div class="row">
              <div class="col-xs-12">
                <label class="control-label">Laboratory Results</label>
                <%= text_area_tag "patient[appointments][medical_records][laboratory_results]", "", :class => "form-control", :style => "height: 100px;" %>
              </div>
            </div>
          </div>

          <div class="form-group">
            <div class="row">
              <div class="col-xs-12">
                <label class="control-label">Physical Examinations</label>
                <%= text_area_tag "patient[appointments][medical_records][physical_examinations]", "", :class => "form-control", :style => "height: 100px;" %>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="hr-line-dashed"></div>

      <h3>Doctor's Remarks</h3>

      <div class="row">
        <div class="col-lg-12">
          <div class="form-group">
            <div class="row">
              <div class="col-xs-6">
                <label class="control-label">RETURN VISIT</label>
                <div id="fld-return-visit">
                  <div class="input-group date">
                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span><%= text_field_tag "patient[appointments][medical_records][return_visit]", "",  autofocus: true, class: "form-control" %>
                  </div>
                </div>
              </div>
              <div class="col-xs-6">
                <label class="control-label">RECORD DATE</label>
                <div id="fld-record-date">
                  <div class="input-group date">
                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span><%= text_field_tag "patient[appointments][record_date]", DateTime.now.to_date.strftime('%d/%m/%Y'),  autofocus: true, class: "form-control" %>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <div class="form-group">
            <div class="row">
              <div class="col-xs-12">
                <label class="control-label">MANAGEMENT</label>
                <%= text_area_tag "patient[appointments][management]", "", :class => "form-control", :style => "height: 100px;" %>
              </div>
            </div>
          </div>

          <div class="form-group">
            <div class="row">
              <div class="col-xs-12">
                <label class="control-label">RECOMMENDATIONS</label>
                <%= text_area_tag "patient[appointments][recommendations]", "", :class => "form-control", :style => "height: 100px;" %>
              </div>
            </div>
          </div>
            
         <div class="form-group">
            <div class="row">
              <div class="col-xs-12">
                <label class="control-label">SUMMARY</label>
                <%= text_area_tag "patient[appointments][summary]", "", :class => "form-control", :style => "height: 100px;" %>
              </div>
            </div>
          </div>
        </div>
      </div>

    </div>
    <div class="col-lg-2">
      <div class="text-center">
        <div style="margin-top: 20px">
          <i class="fa fa-sign-in" style="font-size: 180px;color: #e5e5e5 "></i>
        </div>
      </div>
    </div>
  </div>
</fieldset>
<h1>Patient</h1>
